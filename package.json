{
  "name": "select-vue3",
  "version": "0.0.10",
  "type": "module",
  "description": "A Select component for vue 3 ",
  "private": false,
  "files": [
    "dist",
    "src/components/"
  ],
  "main": "./dist/select-vue3.umd.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/select-vue3.js",
      "require": "./dist/select-vue3.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "serve": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dodaxx/select-vue3"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "select-vue",
    "library-vue"
  ],
  "author": "Dodaxx",
  "dependencies": {
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "sass": "^1.64.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.1",
    "vue-tsc": "^1.8.5"
  }
}
